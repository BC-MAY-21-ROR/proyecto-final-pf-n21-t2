<%= render "layouts/menu"%>

<% @video.each do |v|%>
      <div class="card col-md-3 col-sm-12 p-0">
        <iframe src="https://www.youtube.com/embed/<%= v.link %>" title="<%= v.title %>" frameborder="0"></iframe>
        <div class="card-body">
          <h5 class="card-title"><%= v.title %></h5>
          <p class="card-text description"><%= v.description %></p>
          <%= link_to "Take class", "#", class: "btn" %>
        </div>
        <div class="card-footer">
          <small class="text-muted">
            <span class="material-icons icon-profile">account_circle</span>
            <span class="owner-name">Mauricio Navarro</span>
          </small>
        </div>
      </div>
    <% end%>

