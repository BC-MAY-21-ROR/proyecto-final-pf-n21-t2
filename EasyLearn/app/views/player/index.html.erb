<%= render "layouts/menu"%>

<div class="container-fluid row g-0 d-flex mt-5 player-wrapper">
	<div class="col-sm-6 col-md-8">
		<% @videoPlayer.each do |vp|%>
			<div class="video">
				<iframe src="https://www.youtube.com/embed/<%= vp.link %>" title="<%= vp.title %>" allowfullscreen frameborder="0"></iframe>
			</div>
		<% end %>
	</div>
</div>
